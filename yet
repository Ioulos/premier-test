[32m+[m[32mimportScripts('./build/sw-toolbox.js');[m
[32m+[m
[32m+[m[32mself.toolbox.options.cache = {[m
[32m+[m[32m  name: 'ionic-cache'[m
[32m+[m[32m};[m
