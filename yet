[32m+[m[32m  getphoto(){[m
[32m+[m
[32m+[m[41m  [m	[32mconst options: CameraOptions = {[m
[32m+[m[32m  quality: 100,[m
[32m+[m[32m  destinationType: this.camera.DestinationType.DATA_URL,[m
[32m+[m[32m  encodingType: this.camera.EncodingType.JPEG,[m
[32m+[m[32m  mediaType: this.camera.MediaType.PICTURE[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32mthis.camera.getPicture(options).then((imageData) => {[m
[32m+[m[32m // imageData is either a base64 encoded string or a file URI[m
[32m+[m[32m // If it's base64:[m
[32m+[m[32m this.base64Image = 'data:image/jpeg;base64,' + imageData;[m
[32m+[m[32m}, (err) => {[m
[32m+[m[32m // Handle error[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m  }[m
