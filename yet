[32m+[m[32m    { provide: ErrorHandler, useClass: IonicErrorHandler },[m
[32m+[m[32m    MovieApiProvider,[m
[32m+[m[32m    FavoriteMovieProvider,[m
[32m+[m[32m    Camera,[m
[32m+[m[32m    Flashlight[m
[32m+[m[32m  ][m
[32m+[m[32m})[m
[32m+[m[32mexport class AppModule {}[m
