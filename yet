[1m+++ b/src/providers/favorite-movie/favorite-movie.ts[m
[36m@@ -0,0 +1,53 @@[m
[32m+[m[32mimport { Injectable } from "@angular/core";[m
[32m+[m[32mimport { Storage } from "@ionic/storage";[m
[32m+[m[32mimport { IMovie } from "../../interface/IMovie";[m
[32m+[m
[32m+[m[32mconst MOVIE_KEY = "movie_";[m
[32m+[m
[32m+[m[32m@Injectable()[m
[32m+[m[32mexport class FavoriteMovieProvider {[m
[32m+[m[32m  constructor(private storage: Storage) {[m
[32m+[m[32m    console.log("Hello UserPreferencesProvider Provider");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  addFavoriteMovie(movie: IMovie) {[m
[32m+[m[32m    this.storage.set(this.getMovieKey(movie), JSON.stringify(movie));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  removeFavoriteMovie(movie: IMovie) {[m
[32m+[m[32m    this.storage.remove(this.getMovieKey(movie));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  isFavortieMovie(movie: IMovie) {[m
[32m+[m[32m    return this.storage.get(this.getMovieKey(movie));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  toogleFavoriteMovie(movie: IMovie) {[m
[32m+[m[32m    this.isFavortieMovie(movie).then([m
[32m+[m[32m      isFavorite =>[m
[32m+[m[32m        isFavorite[m
[32m+[m[32m          ? this.removeFavoriteMovie(movie)[m
[32m+[m[32m          : this.addFavoriteMovie(movie)[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getMovieKey(movie: IMovie) {[m
[32m+[m[32m    return MOVIE_KEY + movie.id.toString();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getFavoriteMovies(): Promise<IMovie[]> {[m
[32m+[m[32m    return new Promise(resolve => {[m
[32m+[m[32m      let results: IMovie[] = [];[m
[32m+[m[32m      this.storage[m
[32m+[m[32m        .keys()[m
[32m+[m[32m        .then(keys =>[m
[32m+[m[32m          keys[m
[32m+[m[32m            .filter(key => key.includes(MOVIE_KEY))[m
[32m+[m[32m            .forEach(key =>[m
[32m+[m[32m              this.storage.get(key).then(data => results.push(JSON.parse(data)))[m
[32m+[m[32m            )[m
[32m+[m[32m        );[m
[32m+[m[32m      return resolve(results);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
\ No newline at end of file[m
[1mdiff --git a/src/providers/movie-api/movie-api.ts b/src/providers/movie-api/movie-api.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..da5024d[m
[1m--- /dev/null[m
[1m+++ b/src/providers/movie-api/movie-api.ts[m
[36m@@ -0,0 +1,26 @@[m
[32m+[m[32mimport { Injectable } from "@angular/core";[m
[32m+[m[32mimport { HttpClient } from "@angular/common/http";[m
[32m+[m[32mimport { Platform } from "ionic-angular";[m
[32m+[m[32mimport { Observable } from "rxjs/Rx";[m
[32m+[m[32mimport { IMovie } from "../../interface/IMovie";[m
[32m+[m
[32m+[m[32m@Injectable()[m
[32m+[m[32mexport class MovieApiProvider {[m
[32m+[m[32m  private baseUrl: string = "../../assets/api/movies.json";[m
[32m+[m
[32m+[m[32m  movies: IMovie[];[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    private readonly http: HttpClient,[m
[32m+[m[32m    private readonly platform: Platform[m
[32m+[m[32m  ) {[m
[32m+[m[32m    console.log("Hello MovieApiProvider Provider");[m
[32m+[m[32m    if (this.platform.is("cordova") && this.platform.is("android")) {[m
[32m+[m[32m      this.baseUrl = "/android_asset/www/assets/api/movies.json";[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getMovies(): Observable<any> {[m
[32m+[m[32m    return this.http.get(`${this.baseUrl}`);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
