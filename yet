[32m+[m[32mimport { FavoriteMovieProvider } from "../../providers/favorite-movie/favorite-movie";[m
[32m+[m[32mimport { IMovie } from "../../interface/IMovie";[m
[32m+[m[32mimport { Component } from "@angular/core";[m
[32m+[m[32mimport { IonicPage, NavController, NavParams } from "ionic-angular";[m
[32m+[m
[32m+[m[32m@IonicPage()[m
[32m+[m[32m@Component({[m
[32m+[m[32m  selector: "page-movie-detail",[m
[32m+[m[32m  templateUrl: "movie-detail.html"[m
[32m+[m[32m})[m
[32m+[m[32mexport class MovieDetailPage {[m
[32m+[m[32m  movie: IMovie;[m
[32m+[m[32m  isFavorite: boolean = false;[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    public navCtrl: NavController,[m
[32m+[m[32m    public navParams: NavParams,[m
[32m+[m[32m    private favoriteMovieProvider: FavoriteMovieProvider[m
[32m+[m[32m  ) {}[m
[32m+[m
[32m+[m[32m  ionViewDidLoad() {[m
[32m+[m[32m    this.movie = this.navParams.data;[m
[32m+[m[32m    this.favoriteMovieProvider[m
[32m+[m[32m      .isFavortieMovie(this.movie)[m
[32m+[m[32m      .then(value => (this.isFavorite = value));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  toggleFavorite(): void {[m
[32m+[m[32m    this.isFavorite = !this.isFavorite;[m
[32m+[m[32m    this.favoriteMovieProvider.toogleFavoriteMovie(this.movie);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
