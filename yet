[32m+[m[32m    private movieApiProvider: MovieApiProvider[m
[32m+[m[32m  ) {}[m
[32m+[m
[32m+[m[32m  ionViewDidLoad() {[m
[32m+[m[32m    this.movieApiProvider.getMovies().subscribe(data => {[m
[32m+[m[32m      this.movies = data;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  goToDetail(movie: IMovie) {[m
[32m+[m[32m    this.navCtrl.push(MovieDetailPage, movie);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
