[32m+[m[32mimport { Component } from '@angular/core';[m
[32m+[m[32mimport { IonicPage, NavController, NavParams } from 'ionic-angular';[m
[32m+[m[32mimport { Camera, CameraOptions } from '@ionic-native/camera';[m
[32m+[m[32mimport { Flashlight } from '@ionic-native/flashlight';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Generated class for the CameraPage page.[m
[32m+[m[32m *[m
[32m+[m[32m * See https://ionicframework.com/docs/components/#navigation for more info on[m
[32m+[m[32m * Ionic pages and navigation.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m@IonicPage()[m
[32m+[m[32m@Component({[m
[32m+[m[32m  selector: 'page-camera',[m
[32m+[m[32m  templateUrl: 'camera.html',[m
[32m+[m[32m})[m
[32m+[m[32mexport class CameraPage {[m
[32m+[m
[32m+[m[32m  constructor(public navCtrl: NavController, public navParams: NavParams, private camera: Camera, private flashlight: Flashlight) {[m
[32m+[m[32m  }[m
[32m+[m[32m base64Image:string;[m
[32m+[m[32m  ionViewDidLoad() {[m
[32m+[m[32m    console.log('ionViewDidLoad CameraPage');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getphoto(){[m
[32m+[m
[32m+[m[41m  [m	[32mconst options: CameraOptions = {[m
[32m+[m[32m  quality: 100,[m
[32m+[m[32m  destinationType: this.camera.DestinationType.DATA_URL,[m
[32m+[m[32m  encodingType: this.camera.EncodingType.JPEG,[m
[32m+[m[32m  mediaType: this.camera.MediaType.PICTURE[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32mthis.camera.getPicture(options).then((imageData) => {[m
[32m+[m[32m // imageData is either a base64 encoded string or a file URI[m
[32m+[m[32m // If it's base64:[m
[32m+[m[32m this.base64Image = 'data:image/jpeg;base64,' + imageData;[m
[32m+[m[32m}, (err) => {[m
[32m+[m[32m // Handle error[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  light(){[m
[32m+[m	[32mthis.flashlight.switchOn()}[m
[32m+[m[32m}[m
[32m+[m
