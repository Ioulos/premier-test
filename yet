[1mdiff --git a/src/pages/list/list.ts b/src/pages/list/list.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..a421ca9[m
[1m--- /dev/null[m
[1m+++ b/src/pages/list/list.ts[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32mimport { Component } from '@angular/core';[m
[32m+[m[32mimport { NavController, NavParams } from 'ionic-angular';[m
[32m+[m
[32m+[m[32m@Component({[m
[32m+[m[32m  selector: 'page-list',[m
[32m+[m[32m  templateUrl: 'list.html'[m
[32m+[m[32m})[m
[32m+[m[32mexport class ListPage {[m
[32m+[m[32m  selectedItem: any;[m
[32m+[m[32m  icons: string[];[m
[32m+[m[32m  items: Array<{title: string, note: string, icon: string}>;[m
[32m+[m
[32m+[m[32m  constructor(public navCtrl: NavController, public navParams: NavParams) {[m
[32m+[m[32m    // If we navigated to this page, we will have an item available as a nav param[m
[32m+[m[32m    this.selectedItem = navParams.get('item');[m
[32m+[m
[32m+[m[32m    // Let's populate this page with some filler content for funzies[m
[32m+[m[32m    this.icons = ['flask', 'wifi', 'beer', 'football', 'basketball', 'paper-plane',[m
[32m+[m[32m    'american-football', 'boat', 'bluetooth', 'build'];[m
[32m+[m
[32m+[m[32m    this.items = [];[m
[32m+[m[32m    for (let i = 1; i < 11; i++) {[m
[32m+[m[32m      this.items.push({[m
[32m+[m[32m        title: 'Item ' + i,[m
[32m+[m[32m        note: 'This is item #' + i,[m
[32m+[m[32m        icon: this.icons[Math.floor(Math.random() * this.icons.length)][m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  itemTapped(event, item) {[m
[32m+[m[32m    // That's right, we're pushing to ourselves![m
[32m+[m[32m    this.navCtrl.push(ListPage, {[m
[32m+[m[32m      item: item[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
