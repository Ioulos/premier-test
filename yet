[36m@@ -0,0 +1,51 @@[m
[32m+[m[32mimport { CameraPage } from "../pages/camera/camera";[m
[32m+[m[32mimport { MovieListPage } from "../pages/movie-list/movie-list";[m
[32m+[m[32mimport { MyMoviesPage } from "../pages/my-movies/my-movies";[m
[32m+[m[32mimport { GitTestPage } from "../pages/git-test/git-test";[m
[32m+[m
[32m+[m[32mimport { Component, ViewChild } from "@angular/core";[m
[32m+[m[32mimport { Nav, Platform } from "ionic-angular";[m
[32m+[m[32mimport { StatusBar } from "@ionic-native/status-bar";[m
[32m+[m[32mimport { SplashScreen } from "@ionic-native/splash-screen";[m
[32m+[m
[32m+[m[32m@Component({[m
[32m+[m[32m  templateUrl: "app.html"[m
[32m+[m[32m})[m
[32m+[m[32mexport class MyApp {[m
[32m+[m[32m  @ViewChild(Nav) nav: Nav;[m
[32m+[m
[32m+[m[32m  rootPage: any = MyMoviesPage;[m
[32m+[m
[32m+[m[32m  pages: Array<{ title: string; component: any }>;[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    public platform: Platform,[m
[32m+[m[32m    public statusBar: StatusBar,[m
[32m+[m[32m    public splashScreen: SplashScreen[m
[32m+[m[32m  ) {[m
[32m+[m[32m    this.initializeApp();[m
[32m+[m
[32m+[m[32m    // used for an example of ngFor and navigation[m
[32m+[m[32m    this.pages = [[m
[32m+[m[32m      { title: "Home", component: MyMoviesPage },[m
[32m+[m[32m      { title: "Movie List", component: MovieListPage },[m
[32m+[m[32m      { title: "camera", component: CameraPage },[m
[32m+[m[32m      { title: "GitTest", component: GitTestPage }[m
[32m+[m[32m    ];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  initializeApp() {[m
[32m+[m[32m    this.platform.ready().then(() => {[m
[32m+[m[32m      // Okay, so the platform is ready and our plugins are available.[m
[32m+[m[32m      // Here you can do any higher level native things you might need.[m
[32m+[m[32m      this.statusBar.styleDefault();[m
[32m+[m[32m      this.splashScreen.hide();[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  openPage(page) {[m
[32m+[m[32m    // Reset the content nav to have just this page[m
[32m+[m[32m    // we wouldn't want the back button to show in this scenario[m
[32m+[m[32m    this.nav.setRoot(page.component);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m[41m [m
\ No newline at end of file[m
