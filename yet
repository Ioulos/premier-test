[36m@@ -0,0 +1,19 @@[m
[32m+[m[32m<ion-menu [content]="content">[m
[32m+[m[32m  <ion-header>[m
[32m+[m[32m    <ion-toolbar>[m
[32m+[m[32m      <ion-title>Menu</ion-title>[m
[32m+[m[32m    </ion-toolbar>[m
[32m+[m[32m  </ion-header>[m
[32m+[m
[32m+[m[32m  <ion-content>[m
[32m+[m[32m    <ion-list>[m
[32m+[m[32m      <button menuClose ion-item *ngFor="let p of pages" (click)="openPage(p)">[m
[32m+[m[32m        {{p.title}}[m
[32m+[m[32m      </button>[m
[32m+[m[32m    </ion-list>[m
[32m+[m[32m  </ion-content>[m
[32m+[m
[32m+[m[32m</ion-menu>[m
